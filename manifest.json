{
    "manifest_version": 3,
    "name": "TabTracker",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "images/icon128.png"
    },
    "author": "parmsam",
    "permissions": [
        "tabs",
        "clipboardWrite"
    ],
    "description": "Save and copy your tab links.",
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}